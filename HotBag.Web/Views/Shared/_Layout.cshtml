<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HotBag.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />


    <!-- CSS Files -->
    <link href="~/area/public/css/paper-bootstrap-wizard.css" rel="stylesheet" />

    <!-- CSS Just for demo purpose, don't include it in your project -->
    <link href="~/area/public/css/demo.css" rel="stylesheet" />

    <!-- Fonts and Icons -->
    @*<link href="https://netdna.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.css" rel="stylesheet">*@
   @* <link href='https://fonts.googleapis.com/css?family=Muli:400,300' rel='stylesheet' type='text/css'>*@
    <link href="~/area/public/css/themify-icons.css" rel="stylesheet" />

</head>
<body> 
    <div class="image-container set-full-height" style="background-image: url('/area/public/img/paper-1.jpeg')">

        @RenderBody() 

        <div class="footer">
            <div class="container text-center">
                Made with <i class="fa fa-heart heart"></i> by <a href="https://www.github.com/pradeept95" target="_blank">Pradeep Raj Thapaliya</a>. Free download <a href="https://www.creative-tim.com/product/paper-bootstrap-wizard" target="_blank">here.</a>
            </div>
        </div>

    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <!--   Core JS Files   -->
    <script src="~/area/public/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>

    <!--  Plugin for the Wizard -->
    <script src="~/area/public/js/demo.js" type="text/javascript"></script>
    <script src="~/area/public/js/paper-bootstrap-wizard.js" type="text/javascript"></script>
    <script src="~/area/public/js/jquery.validate.min.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
